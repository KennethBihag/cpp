{
    "cmake.configureOnOpen": false,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.browse.path": [
        "${workspaceFolder}"
    ],
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**"
    ],
    "C_Cpp.default.defines": [
        "ALL_CHALLENGES",
        "CODEIUM_GEN"
    ],
    "custom": {
        "binDir": "${workspaceFolder}/bin",
        "staticLib": "${workspaceFolder}/lib/static",
        "dynamicLib": "${workspaceFolder}/lib"
    },
    "files.associations": {
        "*.embeddedhtml": "html",
        "threading2.h": "c",
        "winthreading.h": "c",
        "string.h": "c",
        "pthread.h": "c",
